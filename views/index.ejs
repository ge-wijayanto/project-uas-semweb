      <a href="/coupe_roadster_cars">
        <div class="listings-card">
          <img src="images/coupe-cars.png" alt="car1" class="car-image">
          <h2 class="title-font">Coupe/Roadster</h2>
        </div>
      </a>

      <a href="/old_cars">
        <div class="listings-card">
          <img src="images/old-cars.png" alt="car1" class="car-image">
          <h2 class="title-font">Old Cars</h2>
        </div>
